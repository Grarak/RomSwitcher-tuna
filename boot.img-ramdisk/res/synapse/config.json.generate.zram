#!/sbin/busybox sh

cat << CTAG
{
    name:Z-RAM,
    elements:[
        { STitleBar:{
		title:"Swappines"
        }},
	{ SSeekBar:{
       		title:"Swappines",
                min:0, max:100, unit:"%", step:5,
                default:`cat /proc/sys/vm/swappiness`,
                action:"generic /proc/sys/vm/swappiness"
	}},
        { STitleBar:{
		title:"Z-RAM"
        }},
	{ SSeekBar:{
       		title:"Disk 1",
                min:0, max:500, unit:"MB", step:10,
                default:500,
                action:"zram zram0"
	}},
    ]
}
CTAG
